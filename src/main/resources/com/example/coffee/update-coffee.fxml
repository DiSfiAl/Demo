<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.coffee.UpdateCoffeeController">
    <Label text="Coffee Name:"/>
    <TextField fx:id="coffeeNameField"/>
    <Label text="Sort:"/>
    <TextField fx:id="sortField"/>
    <Label text="Type:"/>
    <TextField fx:id="typeField"/>
    <Label text="Package Type:"/>
    <TextField fx:id="packageTypeField"/>
    <Button text="Find Coffee" onAction="#findCoffee"/>
</VBox>
